{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "rewrites": [
        {
          "source": "/app/**",
          "function": "app"
        }
      ]
    }
  ]
}
